CREATE table noticeBoard(           
   option1_id      VARCHAR2(20),   --대분류id  
   option2_id      VARCHAR2(20),   --소분류id   
   notice_subject  VARCHAR2(4000), --제목
   notice_content  VARCHAR2(4000), --내용
   notice_picture  VARCHAR2(4000), --사진
   logtime        DATE DEFAULT SYSDATE
);

